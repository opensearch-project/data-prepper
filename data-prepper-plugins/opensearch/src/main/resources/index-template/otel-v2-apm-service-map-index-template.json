{
  "version": 0,
  "template": {
    "mappings": {
      "date_detection": false,
      "dynamic_templates": [
        {
          "long_source_node_group_by_attributes": {
            "path_match": "sourceNode.groupByAttributes.*",
            "match_mapping_type": "long",
            "mapping": {
              "type": "long"
            }
          }
        },
        {
          "double_source_node_group_by_attributes": {
            "path_match": "sourceNode.groupByAttributes.*",
            "match_mapping_type": "double",
            "mapping": {
              "type": "double"
            }
          }
        },
        {
          "string_source_node_group_by_attributes": {
            "path_match": "sourceNode.groupByAttributes.*",
            "match_mapping_type": "string",
            "mapping": {
              "ignore_above": 256,
              "type": "keyword"
            }
          }
        },
        {
          "long_target_node_group_by_attributes": {
            "path_match": "targetNode.groupByAttributes.*",
            "match_mapping_type": "long",
            "mapping": {
              "type": "long"
            }
          }
        },
        {
          "double_target_node_group_by_attributes": {
            "path_match": "targetNode.groupByAttributes.*",
            "match_mapping_type": "double",
            "mapping": {
              "type": "double"
            }
          }
        },
        {
          "string_target_node_group_by_attributes": {
            "path_match": "targetNode.groupByAttributes.*",
            "match_mapping_type": "string",
            "mapping": {
              "ignore_above": 256,
              "type": "keyword"
            }
          }
        },
        {
          "long_source_operation_attributes": {
            "path_match": "sourceOperation.attributes.*",
            "match_mapping_type": "long",
            "mapping": {
              "type": "long"
            }
          }
        },
        {
          "double_source_operation_attributes": {
            "path_match": "sourceOperation.attributes.*",
            "match_mapping_type": "double",
            "mapping": {
              "type": "double"
            }
          }
        },
        {
          "string_source_operation_attributes": {
            "path_match": "sourceOperation.attributes.*",
            "match_mapping_type": "string",
            "mapping": {
              "ignore_above": 256,
              "type": "keyword"
            }
          }
        },
        {
          "long_target_operation_attributes": {
            "path_match": "targetOperation.attributes.*",
            "match_mapping_type": "long",
            "mapping": {
              "type": "long"
            }
          }
        },
        {
          "double_target_operation_attributes": {
            "path_match": "targetOperation.attributes.*",
            "match_mapping_type": "double",
            "mapping": {
              "type": "double"
            }
          }
        },
        {
          "string_target_operation_attributes": {
            "path_match": "targetOperation.attributes.*",
            "match_mapping_type": "string",
            "mapping": {
              "ignore_above": 256,
              "type": "keyword"
            }
          }
        }
      ],
      "_source": {
        "enabled": true
      },
      "properties": {
        "eventType": {
          "type": "keyword"
        },
        "sourceNode": {
          "properties": {
            "type": {
              "type": "keyword"
            },
            "keyAttributes": {
              "properties": {
                "environment": {
                  "type": "keyword"
                },
                "name": {
                  "type": "keyword"
                }
              }
            },
            "groupByAttributes": {
              "type": "object",
              "dynamic": "true"
            }
          }
        },
        "targetNode": {
          "properties": {
            "type": {
              "type": "keyword"
            },
            "keyAttributes": {
              "properties": {
                "environment": {
                  "type": "keyword"
                },
                "name": {
                  "type": "keyword"
                }
              }
            },
            "groupByAttributes": {
              "type": "object",
              "dynamic": "true"
            }
          }
        },
        "sourceOperation": {
          "properties": {
            "name": {
              "type": "keyword"
            },
            "attributes": {
              "type": "object",
              "dynamic": "true"
            }
          }
        },
        "targetOperation": {
          "properties": {
            "name": {
              "type": "keyword"
            },
            "attributes": {
              "type": "object",
              "dynamic": "true"
            }
          }
        },
        "nodeConnectionHash": {
          "type": "keyword"
        },
        "operationConnectionHash": {
          "type": "keyword"
        },
        "timestamp": {
          "type": "date",
          "format": "strict_date_optional_time||epoch_millis"
        }
      }
    }
  }
}