{
  "mappings": [
    {
      "source": "sourceField",
      "targets": [
        {
          "target": "targetField",
          "regex": {
            "exact": true,
            "patterns": {
              "^key[0-4]$": "matched_single_digit",
              "^key[5-9]$": "matched_single_digit_high",
              "^key\\d{2}$": "matched_double_digit",
              "^special_.*$": "matched_special"
            }
          }
        }
      ]
    }
  ]
}